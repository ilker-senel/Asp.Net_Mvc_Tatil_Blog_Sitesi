@using TravelTripProje.Models.Sınıflar
@model List<Blog>
<div class="box1-left">
    @foreach (var x in Model)
    {
        <div class="bot-img">
            <img src="@x.BlogImage" style="width:120px; height:65px" alt="" />
        </div>
        <div class="bot-desc1">
            <p><a href="/Blog/BlogDetay/@x.Id" style="color:inherit">@x.Aciklama.Substring(0, 70)...</a></p>
        </div>
        <div class="clear"></div>
    }


</div>
